---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Nathan McGinness</title>
    <link href="http://njmcgee.github.com/atom.xml" rel="self"/>
    <link href="http://njmcgee.github.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://njmcgee.github.com/</id>
    
    {% for post in site.posts %}
    <entry>
        <title>{{ post.title | h }}</title>
        {% for category in post.categories %}
        <category term="{{category}}" scheme="http://www.sixapart.com/ns/types#tag" />
        {% endfor %}
        <link href="http://njmcgee.github.com{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>njmcgee.github.com:{{ post.id }}</id>
        <content type="html">
          {% if post.image-path %}
           <img src="http://njmcgee.github.com{{ post.image-path }}"/ >
          {% endif %}
          {% if post.external %}
            {{ post.summary }}    
          {% else %}
            {{ post.content }}          
          {% endif %}
          
        </content>
        <author>
            <name>{{post.author_name}}</name>
            <uri>{{post.author_uri}}</uri>
        </author>
    </entry>
    {% endfor %}
</feed>